<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="easypdf_grid_print_invoices" class="Magento\Sales\Ui\Component\Control\PdfAction">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="pdfAction" xsi:type="string">vnecoms_pdfpro/pdfpro_order/printinvoices</item>
                    </item>
                </argument>
                <settings>
                    <type>easypdf_grid_print_invoices</type>
                    <label translate="true">Easy PDF - Print Invoices</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
