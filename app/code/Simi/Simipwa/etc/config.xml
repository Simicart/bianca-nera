<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <simipwa>
            <homescreen>
                <homescreen_enable>1</homescreen_enable>
                <app_name>Simicart PWA</app_name>
                <app_short_name>Simicart PWA | Progressive Web App</app_short_name>
                <home_screen_icon>https://www.simicart.com/skin/frontend/default/simicart2.0/css/2017/images/icon-logo.png</home_screen_icon>
            </homescreen>
            <notification>
                <enable>1</enable>
                <icon_url>https://www.simicart.com/skin/frontend/default/simicart2.0/css/2017/images/icon-logo.png</icon_url>
            </notification>
            <general>
                <pwa_enable>0</pwa_enable>
                <dashboard_url>https://www.simicart.com</dashboard_url>
                <pwa_mobile_only>1</pwa_mobile_only>
                <dynamic_rendering>0</dynamic_rendering>
            </general>
            <pwa_header>
                <bianca_header_phone>+0965 555 5455 731</bianca_header_phone>
                <bianca_header_sale_title>Sale up to 50%</bianca_header_sale_title>
                <bianca_header_sale_link>#</bianca_header_sale_link>
                <bianca_header_storelocator>#</bianca_header_storelocator>
            </pwa_header>
            <pwa_footer>
                <bianca_footer_phone>+44 345 678 9009</bianca_footer_phone>
                <bianca_footer_email>Example@gmail.com</bianca_footer_email>
                <bianca_footer_facebook>https://facebook.com</bianca_footer_facebook>
                <bianca_footer_instagram>https://instagram.com</bianca_footer_instagram>
                <bianca_footer_twitter>https://twitter.com</bianca_footer_twitter>
                <bianca_footer_linkedin>https://linkedin.com</bianca_footer_linkedin>
                <bianca_footer_google>https://google.com</bianca_footer_google>
            </pwa_footer>
        </simipwa>
    </default>
</config>