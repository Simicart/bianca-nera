<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <container name="listing_top">
        <massaction name="listing_massaction">
            <action name="pdfpro_print_order">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">pdfpro_print_order</item>
                        <item name="label" xsi:type="string" translate="true">Print Orders</item>
                        <item name="url" xsi:type="url" path="sales/order/pdforders"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </container>
</listing>
